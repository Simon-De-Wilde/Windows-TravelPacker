                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <Grid.Background>
                    <AcrylicBrush TintOpacity="0.05"
                                  TintColor="Black" />
                </Grid.Background>

                <!--Table header-->
                <Border x:Name="th_pl"
                        BorderBrush="Black"
                        BorderThickness="0,0,0,1"
                        Grid.Column="0"
                        Grid.Row="0">
                    <TextBlock Text="Packing list"
                               HorizontalAlignment="Center"
                               Padding="25 0 25 0"
                               Style="{ThemeResource TitleTextBlockStyle}" />
                </Border>
                <Border x:Name="th_t"
                        BorderBrush="Black"
                        BorderThickness="0,0,0,1"
                        Grid.Column="1"
                        Grid.Row="0">
                    <TextBlock Text="Tasks"
                               HorizontalAlignment="Center"
                               Padding="25 0 25 0"
                               Style="{ThemeResource TitleTextBlockStyle}" />
                </Border>

                <Border x:Name="th_i"
                        BorderBrush="Black"
                        BorderThickness="0,0,0,1"
                        Grid.Column="2"
                        Grid.Row="0">
                    <TextBlock Text="Itinerary"
                               HorizontalAlignment="Center"
                               Padding="25 0 25 0"
                               Style="{ThemeResource TitleTextBlockStyle}" />
                </Border>

                <!-- List of Items -->

                <ScrollViewer x:Name="SV_items"
                              Grid.Column="0"
                              Grid.Row="1"
                              BorderBrush="Black"
                              BorderThickness="0 0 1 0">
                    <StackPanel>
                        <ListView x:Name="ItemsLV"
                                  ItemsSource="{x:Bind ViewModel.Travel.Categories}">

                            <ListView.ItemContainerStyle>
                                <Style TargetType="ListViewItem">
                                    <Setter Property="Padding"
                                            Value="0" />
                                    <Setter Property="Margin"
                                            Value="0" />
                                    <Setter Property="HorizontalContentAlignment"
                                            Value="Stretch" />
                                </Style>
                            </ListView.ItemContainerStyle>

                            <ListView.ItemTemplate>
                                <DataTemplate x:DataType="model:Category">
                                    <Grid>
                                        <controls:Expander x:Name="EP_categoryE"
                                                           IsExpanded="True"
                                                           RightTapped="RemoveCategory_Tapped">

                                            <!-- Category -->

                                            <controls:Expander.Header>
                                                <TextBlock HorizontalAlignment="Stretch"
                                                           VerticalAlignment="Center"
                                                           Text="{x:Bind Name}" />
                                            </controls:Expander.Header>

                                            <!-- Category items -->

                                            <StackPanel x:Name="ItemsSP"
                                                        Margin="40,0,0,0">
                                                <ItemsControl ItemsSource="{Binding Items}">
                                                    <ItemsControl.ItemTemplate>
                                                        <DataTemplate x:DataType="model:Item">
                                                            <Grid Height="40"
                                                                  RightTapped="RemoveItem_Tapped">
                                                                <CheckBox x:Name="checkbox_item"
                                                                          Grid.Column="0"
                                                                          HorizontalAlignment="Stretch"
                                                                          VerticalAlignment="Center"
                                                                          Content="{x:Bind DisplayTitle}"
                                                                          IsChecked="{x:Bind Done}"
                                                                          Tapped="CheckItem_Tapped" />
                                                            </Grid>
                                                        </DataTemplate>
                                                    </ItemsControl.ItemTemplate>
                                                </ItemsControl>

                                                <!-- New item field -->

                                                <ItemsControl>
                                                    <Grid x:Name="grid_newItem"
                                                          Height="40">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="auto" />
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="auto" />
                                                        </Grid.ColumnDefinitions>

                                                        <TextBox x:Name="textbox_amount"
                                                                 Grid.Column="0"
                                                                 Text="1"
                                                                 VerticalAlignment="Center"
                                                                 BorderBrush="Transparent" />

                                                        <TextBox x:Name="textbox_newItem"
                                                                 Grid.Column="1"
                                                                 PlaceholderText="Add new Item"
                                                                 VerticalAlignment="Center"
                                                                 BorderBrush="Transparent" />

                                                        <FontIcon x:Name="icon_addItem"
                                                                  Grid.Column="2"
                                                                  FontFamily="Segoe MDL2 Assets"
                                                                  Glyph="&#xE710;"
                                                                  HorizontalAlignment="Right"
                                                                  Margin="10,0,10,0"
                                                                  Tapped="AddItem_Tapped" />
                                                    </Grid>
                                                </ItemsControl>
                                            </StackPanel>
                                        </controls:Expander>

                                        <!-- Category item count -->

                                        <StackPanel x:Name="SP_categoryItemsCount"
                                                    Orientation="Horizontal"
                                                    Margin="10,10,10,0"
                                                    HorizontalAlignment="Right"
                                                    VerticalAlignment="Top">
                                            <TextBlock>
                                                <Run Text="{Binding ItemsDone}"></Run>
                                                <Run Text="/"
                                                     FontWeight="Bold"></Run>
                                                <Run Text="{Binding Items.Count}"
                                                     FontWeight="Bold"></Run>
                                            </TextBlock>
                                        </StackPanel>
                                    </Grid>
                                </DataTemplate>
                            </ListView.ItemTemplate>
                        </ListView>

                        <!-- New category field -->

                        <Grid x:Name="grid_newCategory"
                              Height="40">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>

                            <TextBox x:Name="txt_newCategory"
                                     Grid.Column="0"
                                     VerticalAlignment="Center"
                                     PlaceholderText="Add new category"
                                     Margin="30,0,0,0"
                                     BorderBrush="Transparent" />

                            <StackPanel Orientation="Horizontal"
                                        HorizontalAlignment="Right"
                                        Grid.Column="1">
                                <FontIcon x:Name="icon_addCategory"
                                          Grid.Column="1"
                                          FontFamily="Segoe MDL2 Assets"
                                          Glyph="&#xE710;"
                                          Margin="10,0,10,0"
                                          HorizontalAlignment="Right"
                                          Tapped="AddCategory_Tapped" />
                            </StackPanel>
                        </Grid>
                    </StackPanel>
                </ScrollViewer>

                <!-- List of tasks -->
                <ScrollViewer x:Name="SV_tasks"
                              Grid.Column="1"
                              Grid.Row="1"
                              BorderBrush="Black"
                              BorderThickness="0 0 1 0">
                    <StackPanel>
                        <ListView Padding="10"
                                  Margin="50 0 0 0"
                                  x:Name="listView2"
                                  ItemContainerStyle="{StaticResource ListViewItemContainerStyle1}"
                                  ItemsSource="{x:Bind ViewModel.Travel.Tasks}">
                            <ListView.ItemTemplate>
                                <DataTemplate x:DataType="model:TravelTask">
                                    <StackPanel Orientation="Vertical">
                                        <StackPanel Orientation="Horizontal">
                                            <CheckBox Content="{x:Bind Title}"
                                                      Checked="onItemChecked" />
                                            <Button HorizontalAlignment="Right"
                                                    Margin="200 0 0 0"
                                                    Click="btn_DeleteTask_Click">
                                                <SymbolIcon Symbol="Delete" />
                                            </Button>
                                        </StackPanel>
                                    </StackPanel>
                                </DataTemplate>
                            </ListView.ItemTemplate>
                        </ListView>
                        <Button x:Name="btnAddTask"
                                Click="btn_AddTask_Click"
                                Margin="17,0,17,17">
                            <SymbolIcon Symbol="Add" />
                        </Button>
                    </StackPanel>
                </ScrollViewer>
                <!-- Itinerary -->
                <ScrollViewer x:Name="SV_itinerary"
                              Grid.Row="1"
                              Grid.Column="2">
                    <StackPanel Orientation="Vertical">
                        <ListView x:Name="lv_itinerary"
                                  ItemContainerStyle="{StaticResource ListViewItemContainerStyle1}"
                                  SelectionMode="None"
                                  ItemsSource="{x:Bind ViewModel.Travel.Itineraries}">
                            <ListView.ItemTemplate>
                                <DataTemplate x:DataType="model:ItineraryItem">
                                    <Grid x:Name="GV_itineraryCard"
                                          Background="{ThemeResource SystemAccentColorLight3}"
                                          Padding="10"
                                          Width="500"
                                          Margin="5"
                                          IsRightTapEnabled="True"
                                          IsTapEnabled="False"
                                          RightTapped="GV_itineraryCard_RightTapped">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <StackPanel Orientation="Horizontal"
                                                    Grid.Column="0"
                                                    Grid.Row="0"
                                                    Grid.ColumnSpan="2">
                                            <TextBlock Text="{x:Bind Title}" />
                                            <SymbolIcon Symbol="Accept"
                                                        Margin="5,0,0,0"
                                                        Visibility="{Binding Done, Converter={StaticResource bool2vis}}" />
                                        </StackPanel>
                                        <TextBlock Text="   From:"
                                                   Grid.Column="0"
                                                   Grid.Row="1" />
                                        <TextBlock Text="{x:Bind Start, Converter={StaticResource dateConverter}}"
                                                   Grid.Column="1"
                                                   Grid.Row="1" />
                                        <TextBlock Text="   Until:"
                                                   Grid.Column="0"
                                                   Grid.Row="2" />
                                        <TextBlock Text="{x:Bind End, Converter={StaticResource dateConverter}}"
                                                   Grid.Column="1"
                                                   Grid.Row="2" />

                                    </Grid>
                                </DataTemplate>
                            </ListView.ItemTemplate>
                        </ListView>
                        <Button x:Name="add_itineraryItem"
                                Click="add_itineraryItem_Click"
                                Margin="17">
                            <SymbolIcon Symbol="Add"
                                        VerticalAlignment="Center" />
                        </Button>
                    </StackPanel>
                </ScrollViewer>
                <!--World Icon-->
                <Viewbox x:Name="icon_world"
                         Grid.Column="0"
                         Grid.Row="2"
                         Margin="10"
                         MaxHeight="40"
                         MaxWidth="40">
                    <SymbolIcon Symbol="World" />
                </Viewbox>

                <!--List Icon-->

                <Viewbox x:Name="icon_list"
                         Grid.Column="1"
                         Grid.Row="2"
                         Margin="10"
                         MaxHeight="40"
                         MaxWidth="40">
                    <SymbolIcon Symbol="Bullets" />
                </Viewbox>

                <!--Calendar Icon-->

                <Viewbox x:Name="icon_calendar"
                         Grid.Column="2"
                         Grid.Row="2"
                         Margin="10"
                         MaxHeight="40"
                         MaxWidth="40">
                    <SymbolIcon Symbol="Calendar" />
                </Viewbox>

            </Grid>
        </StackPanel>
    </ScrollViewer>
