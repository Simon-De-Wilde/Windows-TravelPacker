              Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Viewbox x:Name="icon_world"
                     Grid.Column="0"
                     Grid.Row="0"
                     Margin="10"
                     MaxHeight="40"
                     MaxWidth="40">
                <SymbolIcon Symbol="World" />
            </Viewbox>
